# CHABS Production Environment Configuration
NEXT_PUBLIC_APP_NAME=CHABS
NEXT_PUBLIC_VERSION=2.0.0
NEXT_PUBLIC_ENVIRONMENT=production

# Storage Configuration - Using Supabase
